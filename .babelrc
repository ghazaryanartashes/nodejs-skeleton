{
  "presets": ["env", "stage-0"],
  "plugins": [
    ["module-alias", [{
      "src": "./src/app/const",
      "expose": "const"
    }, {
      "src": "./src/app/http/controllers",
      "expose": "controllers"
    }, {
      "src": "./src/app/http/middleware",
      "expose": "middleware"
    }, {
      "src": "./src/app/http/validators",
      "expose": "validators"
    }, {
      "src": "./src/app/http/exceptions",
      "expose": "exceptions"
    }, {
      "src": "./src/app/models",
      "expose": "model"
    }, {
      "src": "./src/app/services",
      "expose": "services"
    }, {
      "src": "./src/lib",
      "expose": "lib"
    }, {
      "src": "./src/config",
      "expose": "config"
    }]]
  ]
}